apiVersion: v1
kind: Secret
metadata:
  name: airthings-secrets
  namespace: airthings
type: Opaque
stringData:
  AIRTHINGS_CLIENT_ID: "your-client-id-here"
  AIRTHINGS_DEVICE_ID: "your-device-id-here"
  AIRTHINGS_SECRET: "your-secret-here"
  POSTGRES_HOST: "simple-postgres.postgres.svc.cluster.local"
  POSTGRES_PORT: "5432"
  POSTGRES_DB: "postgres"
  POSTGRES_USER: "postgres"
  POSTGRES_PASSWORD: "your-postgres-password-here"
